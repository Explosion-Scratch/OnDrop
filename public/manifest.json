{
    "name": "OnDrop",
    "short_name": "OnDrop",
    "display": "standalone",
    "background_color": "#0d0e14",
		"start_url": "./",
		"scope": ".",
		"share_target": {
			"action": "/?share_target",
			"method": "POST",
			"enctype": "multipart/form-data",
			"params": {
				"title": "title",
				"text": "text",
				"url": "url",
				"files": [
					{
						"name": "files",
						"accept": "*/*"
					}
				]
			}
		},
		"shortcuts": [
			{
					"name": "Upload",
					"description": "Upload a file to all clients",
					"url": "/file_upload=1"
			}
		],
		"theme_color": "#0d0e14",
    "description": "A lightning fast, simple online alternative to airdrop!",
